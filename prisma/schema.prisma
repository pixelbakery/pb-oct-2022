datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider      = "prisma-client-js"
  engineType    = "binary"
  binaryTargets = ["darwin", "native", "windows"]
}


model Info {
  id                 String    @id @default(cuid())
  last_image_refresh DateTime
  last_token_refresh DateTime
  
  access_token       String
}

model InstagramLink {
  id         String    @id @default(cuid())
  instagram_id String
  media_type   String
  permalink    String
  media_url    String
  caption      String
}
