{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.taxonomy.seoDescriptionField": "excerpt",
  "frontMatter.taxonomy.seoSlugLength": 75,
  "frontMatter.taxonomy.seoDescriptionLength": 155,
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "_jobs",
      "pageBundle": false,
      "previewPath": "careers",
      "fields": [
        {
          "title": "type",
          "name": "type",
          "type": "string",
          "default": "_jobs",
          "hidden": true
        },
        {
          "title": "Job Title",
          "name": "title",
          "type": "string",
          "single": true,
          "required": true
        },
        {
          "title": "Active Position",
          "name": "active",
          "type": "boolean",
          "default": true
        },
        {
          "title": "Location",
          "name": "location",
          "type": "string",
          "single": true,
          "required": true
        },
        {
          "title": "Cover Image (Optional)",
          "name": "coverImage",
          "type": "string",
          "single": true
        },
        {
          "title": "Commitment",
          "name": "commitment",
          "type": "choice",
          "choices": [
            "",
            "Full-Time",
            "Part-Time",
            "Full/Part-Time",
            "Contractor",
            "Other"
          ]
        },
        {
          "title": "Internship?",
          "name": "internship",
          "type": "boolean"
        },
        {
          "title": "Publishing Date",
          "name": "date",
          "type": "datetime",
          "default": "{{now}}",
          "isPublishDate": true,
          "dateFormat": "YYYY-MM-dd",
          "required": true
        }
      ]
    },
    {
      "name": "_caseStudies",
      "pageBundle": false,
      "previewPath": "work/case-studies",
      "fields": [
        {
          "title": "Client",
          "name": "client",
          "type": "string",
          "single": true,
          "required": true
        },
        {
          "title": "Active",
          "name": "active",
          "type": "boolean",
          "default": true
        },
        {
          "title": "title",
          "name": "title",
          "type": "string",
          "single": true,
          "required": true
        },
        {
          "title": "excerpt",
          "name": "excerpt",
          "type": "string",
          "single": false,
          "required": true
        },
        {
          "title": "Date Published",
          "name": "date",
          "type": "datetime",
          "default": "{{now}}",
          "required": true,
          "isPublishDate": true,
          "dateFormat": "YYYY-MM-dd"
        },
        {
          "title": "Services",
          "name": "tags",
          "type": "taxonomy",
          "taxonomyId": "tags_services",
          "required": true
        },
        {
          "title": "Vimeo ID",
          "description": "The string of numbers at the end of the URL of the vimeo link",
          "name": "vimeoID",
          "type": "string",
          "single": true
        },
        {
          "title": "Video Preview Images",
          "description": "The file name of the preview images. Do not include the extension (.jpg) or the file path.",
          "name": "vimeoPreview",
          "type": "string",
          "single": true,
          "required": true
        },
        {
          "title": "Client Website",
          "name": "website",
          "default": "https://",
          "type": "string",
          "single": true,
          "required": true
        },
        {
          "title": "Logo",
          "description": "Either a PNG or SVG",
          "name": "logo",
          "type": "string",
          "single": true
        },
        {
          "title": "Credits",
          "name": "credits",
          "type": "block",
          "fieldGroup": [
            "credits"
          ]
        },
        {
          "title": "Slug",
          "name": "slug",
          "type": "slug",
          "default": "{{slug}}",
          "editable": true
        },
        {
          "title": "type",
          "name": "type",
          "default": "_caseStudies",
          "type": "string",
          "single": true,
          "required": true,
          "hidden": true
        }
      ]
    },
    {
      "name": "default",
      "pageBundle": false,
      "previewPath": null,
      "fields": [
        {
          "title": "Title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "Description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "Publishing date",
          "name": "date",
          "type": "datetime",
          "default": "{{now}}",
          "isPublishDate": true,
          "dateFormat": "YYYY-MM-dd",
          "required": true
        },
        {
          "title": "Content preview",
          "name": "preview",
          "type": "image"
        },
        {
          "title": "Is in draft",
          "name": "draft",
          "type": "draft",
          "default": false
        },
        {
          "title": "Tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "Categories",
          "name": "categories",
          "type": "categories",
          "taxonomyLimit": 1
        },
        {
          "title": "Slug",
          "name": "slug",
          "type": "slug",
          "editable": true
        }
      ]
    },
    {
      "name": "_madeToOrder",
      "pageBundle": false,
      "previewPath": "education/tutorials",
      "template": "[[workspace]]/.frontmatter/templates/_madeToOrder.mdx",
      "fields": [
        {
          "title": "Cover Image",
          "name": "coverImage",
          "type": "image",
          "isPreviewImage": true
        },
        {
          "title": "Title",
          "name": "title",
          "type": "string",
          "single": true
        },
        {
          "title": "Visible On Site",
          "name": "active",
          "type": "boolean",
          "default": true
        },
        {
          "title": "Draft",
          "name": "draft",
          "type": "draft",
          "default": false
        },
        {
          "title": "Date Published",
          "name": "date",
          "type": "datetime",
          "default": "{{now}}",
          "isPublishDate": true,
          "dateFormat": "YYYY-MM-dd",
          "required": true
        },
        {
          "title": "Category",
          "name": "category",
          "type": "taxonomy",
          "taxonomyId": "categories_programs",
          "taxonomyLimit": 1
        },
        {
          "title": "Video Header Link",
          "name": "video",
          "type": "string",
          "single": true
        },
        {
          "title": "Author",
          "type": "fields",
          "name": "author",
          "fields": [
            {
              "title": "Author Name",
              "name": "name",
              "type": "string",
              "single": true
            },
            {
              "title": "Author URL",
              "name": "url",
              "type": "string",
              "single": true
            }
          ]
        },
        {
          "title": "excerpt",
          "name": "excerpt",
          "type": "string"
        },
        {
          "title": "Tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "File Download Link",
          "name": "downloadLink",
          "type": "file",
          "multiple": false,
          "fileExtensions": []
        },
        {
          "title": "type",
          "name": "type",
          "type": "string",
          "default": "_madeToOrder",
          "hidden": true
        },
        {
          "title": "Slug",
          "name": "slug",
          "type": "slug",
          "editable": true
        }
      ]
    },
    {
      "name": "_posts",
      "pageBundle": false,
      "previewPath": "recipes",
      "template": "[[workspace]]/.frontmatter/templates/_posts.mdx",
      "fields": [
        {
          "title": "Title",
          "name": "title",
          "type": "string",
          "single": true
        },
        {
          "title": "Subtitle",
          "name": "subtitle",
          "type": "string",
          "single": true
        },
        {
          "title": "Author",
          "type": "fields",
          "name": "author",
          "fields": [
            {
              "title": "Author Name",
              "name": "name",
              "type": "dataFile",
              "dataFileId": "teamMembers",
              "dataFileKey": "name",
              "dataFileValue": "name",
              "multiple": false
            },
            {
              "title": "Author Slug",
              "name": "slug",
              "type": "dataFile",
              "dataFileId": "teamMembers",
              "dataFileKey": "slug",
              "dataFileValue": "name",
              "multiple": false
            }
          ]
        },
        {
          "title": "Publishing date",
          "name": "date",
          "type": "datetime",
          "default": "{{now}}",
          "isPublishDate": true,
          "dateFormat": "YYYY-MM-dd",
          "required": true
        },
        {
          "title": "Cover Image",
          "name": "coverImage",
          "type": "image"
        },
        {
          "title": "Video Header Link",
          "name": "video",
          "type": "string",
          "default": "",
          "single": true
        },
        {
          "title": "Is in draft",
          "name": "draft",
          "type": "draft",
          "default": false
        },
        {
          "title": "Categories",
          "name": "categories",
          "type": "categories",
          "taxonomyLimit": 1
        },
        {
          "title": "Tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "Slug",
          "name": "slug",
          "type": "slug",
          "editable": true
        },
        {
          "title": "excerpt",
          "name": "excerpt",
          "type": "string"
        }
      ]
    },
    {
      "name": "_projectFiles",
      "pageBundle": false,
      "fields": [
        {
          "title": "title",
          "name": "title",
          "type": "string",
          "single": true
        },
        {
          "title": "date",
          "name": "date",
          "type": "datetime",
          "default": "{{now}}",
          "isPublishDate": true,
          "dateFormat": "YYYY-MM-dd",
          "required": true
        },
        {
          "title": "active",
          "name": "active",
          "type": "draft",
          "default": false
        },
        {
          "title": "category",
          "name": "category",
          "type": "string",
          "single": true
        },
        {
          "title": "downloadLink",
          "name": "downloadLink",
          "type": "string",
          "single": true
        },
        {
          "title": "fileName",
          "name": "fileName",
          "type": "string",
          "single": true
        },
        {
          "title": "fileSize",
          "name": "fileSize",
          "type": "string",
          "single": true
        },
        {
          "title": "fileType",
          "name": "fileType",
          "type": "string",
          "single": true
        },
        {
          "title": "tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "coverImage",
          "name": "coverImage",
          "type": "string",
          "single": true
        },
        {
          "title": "videoCoverImage",
          "name": "videoCoverImage",
          "type": "boolean"
        },
        {
          "title": "video",
          "name": "video",
          "type": "string",
          "single": true
        },
        {
          "title": "excerpt",
          "name": "excerpt",
          "type": "string"
        },
        {
          "name": "type",
          "type": "string",
          "default": "_projectFile",
          "hidden": true
        }
      ]
    }
  ],
  "frontMatter.preview.host": "http://localhost:3000",
  "frontMatter.framework.id": "next",
  "frontMatter.content.publicFolder": "public",
  "frontMatter.content.pageFolders": [
    {
      "title": "_posts",
      "path": "[[workspace]]/_posts",
      "previewPath": "recipes"
    },
    {
      "title": "_jobs",
      "path": "[[workspace]]/_jobs",
      "previewPath": "careers"
    },
    {
      "title": "_people",
      "path": "[[workspace]]/_people",
      "previewPath": "about"
    },
    {
      "title": "_projectFiles",
      "path": "[[workspace]]/_projectFiles"
    },
    {
      "title": "_madeToOrder",
      "path": "[[workspace]]/_madeToOrder",
      "previewPath": "education/tutorials"
    },
    {
      "title": "_caseStudies",
      "path": "[[workspace]]/_caseStudies",
      "previewPath": "work/case-studies"
    }
  ],
  "frontMatter.taxonomy.customTaxonomy": [
    {
      "id": "socialTaxonomy",
      "options": [
        "Instagram",
        "Facebook",
        "LinkedIn"
      ]
    },
    {
      "id": "categories_programs",
      "options": [
        "After Effects",
        "DaVinci Resolve",
        "Premiere",
        "Procreate",
        "Illustrator"
      ]
    },
    {
      "id": "tags_services",
      "options": [
        "2d Animation",
        "3d Animation",
        "3d Modeling",
        "Animated Explainer",
        "Artificial Reality",
        "Casting & Talent Management",
        "Color Grading",
        "Creative Concepting",
        "Drone / Aerial Videography",
        "Ecommerce",
        "Emerging Media",
        "Film Production",
        "Illustration",
        "Kinetic Typography",
        "Logo Animation",
        "Motion Design",
        "Motion Graphics",
        "Post-Production",
        "Prototyping",
        "Roll-Out Marketing Plan",
        "Scripting & Storyboarding",
        "Set Design",
        "Social Media Toolkits",
        "Sound Design",
        "YouTube Ads",
        "UI / UX Design",
        "Video Editing",
        "Video Production",
        "Web Hosting",
        "Web Design"
      ]
    }
  ],
  "frontMatter.global.activeMode": "",
  "frontMatter.global.modes": [
    {
      "id": "minimal",
      "features": [
        "panel.metadata",
        "panel.seo",
        "panel.contentType",
        "panel.otherActions",
        "panel.actions",
        "dashboard.data.view",
        "dashboard.snippets.manage",
        "dashboard.snippets.view",
        "dashboard.taxonomy.view"
      ]
    }
  ],
  "frontMatter.content.snippets": {
    "Video": {
      "description": "video content",
      "body": "<Video url={'[[url]]'} poster={'[[poster]]'} autoplay={[[autoplay]]} title={'[[title]]'} muted={[[muted]]} loop={[[loop]]} controls={[[controls]]} playsInline={[[playsInline]]} />",
      "fields": [
        {
          "name": "url",
          "title": "Video URL",
          "type": "string",
          "default": "",
          "single": true
        },
        {
          "name": "poster",
          "title": "Poster Image",
          "type": "image"
        },
        {
          "name": "title",
          "title": "Title (optional)",
          "type": "string",
          "single": true
        },
        {
          "name": "muted",
          "title": "Muted",
          "type": "boolean",
          "default": true
        },
        {
          "name": "loop",
          "title": "Loop",
          "type": "boolean",
          "default": true
        },
        {
          "name": "controls",
          "title": "Show Controls",
          "type": "boolean",
          "default": false
        },
        {
          "name": "playsInline",
          "title": "Plays Inline",
          "type": "boolean",
          "default": true
        },
        {
          "name": "autoplay",
          "title": "Auto Play",
          "type": "boolean",
          "default": true
        }
      ]
    }
  },
  "frontMatter.data.files": [
    {
      "id": "teamMembers",
      "title": "Team Members",
      "file": "[[workspace]]/.frontmatter/content/people.json",
      "fileType": "json",
      "labelField": "name",
      "schema": {
        "title": "Team Members",
        "type": "object",
        "required": [
          "name",
          "slug",
          "addLink"
        ],
        "properties": {
          "name": {
            "type": "string",
            "title": "Name"
          },
          "slug": {
            "type": "string",
            "title": "Slug"
          },
          "addLink": {
            "type": "boolean",
            "title": "Link to Bio",
            "default": true
          }
        }
      }
    }
  ],
  "frontMatter.taxonomy.fieldGroups": [
    {
      "id": "credits",
      "labelField": "name",
      "fields": [
        {
          "title": "Name",
          "name": "name",
          "type": "string",
          "single": true
        },
        {
          "title": "Title",
          "name": "title",
          "type": "string",
          "single": true
        },
        {
          "title": "Website",
          "description": "Internal or otherwise.",
          "name": "website",
          "type": "string",
          "default": "/about/",
          "single": true
        }
      ]
    },
    {
      "id": "socials",
      "labelField": "poop",
      "fields": [
        {
          "title": "Platform",
          "name": "platform",
          "type": "choice",
          "choices": [
            "",
            "Instagram",
            "Facebook",
            "LinkedIn"
          ]
        },
        {
          "title": "url",
          "name": "social",
          "type": "string",
          "single": true
        }
      ]
    }
  ],
  "frontMatter.taxonomy.dateFormat": "YYYYYY-MM-dd",
  "frontMatter.content.fmHighlight": false
}