{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.taxonomy.seoDescriptionField": "excerpt",
  "frontMatter.taxonomy.seoSlugLength": 75,
  "frontMatter.taxonomy.seoDescriptionLength": 155,
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "_caseStudies",
      "pageBundle": false,
      "previewPath": "work/case-studies",
      "fields": [
        {
          "title": "Client",
          "name": "client",
          "type": "string",
          "single": true,
          "required": true
        },
        {
          "title": "Active",
          "name": "active",
          "type": "boolean",
          "default": true
        },
        {
          "title": "title",
          "name": "title",
          "type": "string",
          "single": true,
          "required": true
        },
        {
          "title": "excerpt",
          "name": "excerpt",
          "type": "string",
          "single": true,
          "required": true
        },
        {
          "title": "Date Published",
          "name": "date",
          "type": "string",
          "single": true,
          "default": "{{now}}",
          "required": true
        },
        {
          "title": "Services",
          "name": "tags",
          "type": "taxonomy",
          "taxonomyId": "tags_services",
          "required": true
        },
        {
          "title": "Vimeo ID",
          "description": "The string of numbers at the end of the URL of the vimeo link",
          "name": "vimeoID",
          "type": "string",
          "single": true
        },
        {
          "title": "Video Preview Images",
          "description": "The file name of the preview images. Do not include the extension (.jpg) or the file path.",
          "name": "vimeoPreview",
          "type": "string",
          "single": true,
          "required": true
        },
        {
          "title": "Client Website",
          "name": "website",
          "default": "https://",
          "type": "string",
          "single": true,
          "required": true
        },
        {
          "title": "Logo",
          "description": "Either a PNG or SVG",
          "name": "logo",
          "type": "string",
          "single": true,
          "required": true
        },
        {
          "title": "Credits",
          "name": "credits",
          "type": "block",
          "fieldGroup": [
            "credits"
          ]
        },
        {
          "title": "Slug",
          "name": "slug",
          "type": "slug",
          "default": "{{slug}}",
          "editable": true
        },
        {
          "title": "type",
          "name": "type",
          "default": "_caseStudies",
          "type": "string",
          "single": true,
          "required": true,
          "hidden": true
        }
      ]
    },
    {
      "name": "default",
      "pageBundle": false,
      "previewPath": null,
      "fields": [
        {
          "title": "Title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "Description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "Publishing date",
          "name": "date",
          "type": "string",
          "default": "{{now}}",
          "single": true,
          "isPublishDate": true
        },
        {
          "title": "Content preview",
          "name": "preview",
          "type": "image"
        },
        {
          "title": "Is in draft",
          "name": "draft",
          "type": "draft"
        },
        {
          "title": "Tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "Categories",
          "name": "categories",
          "type": "categories",
          "taxonomyLimit": 1
        },
        {
          "title": "Slug",
          "name": "slug",
          "type": "slug",
          "editable": true
        }
      ]
    },
    {
      "name": "_madeToOrder",
      "pageBundle": false,
      "previewPath": "education/tutorials",
      "template": "[[workspace]]/.frontmatter/templates/_madeToOrder.mdx",
      "fields": [
        {
          "title": "Cover Image",
          "name": "coverImage",
          "type": "image",
          "isPreviewImage": true
        },
        {
          "title": "Title",
          "name": "title",
          "type": "string",
          "single": true
        },
        {
          "title": "Visible On Site",
          "name": "active",
          "type": "boolean",
          "default": true
        },
        {
          "title": "Draft",
          "name": "draft",
          "type": "draft",
          "default": true
        },
        {
          "title": "Date Published",
          "name": "date",
          "type": "string",
          "single": true,
          "default": "{{now}}"
        },
        {
          "title": "Category",
          "name": "category",
          "type": "taxonomy",
          "taxonomyId": "categories_programs",
          "taxonomyLimit": 1
        },
        {
          "title": "Video Header Link",
          "name": "video",
          "type": "string",
          "single": true
        },
        {
          "title": "Author",
          "type": "fields",
          "name": "author",
          "fields": [
            {
              "title": "Author Name",
              "name": "name",
              "type": "string",
              "single": true
            },
            {
              "title": "Author URL",
              "name": "url",
              "type": "string",
              "single": true
            }
          ]
        },
        {
          "title": "excerpt",
          "name": "excerpt",
          "type": "string"
        },
        {
          "title": "Tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "File Download Link",
          "name": "downloadLink",
          "type": "file",
          "multiple": false,
          "fileExtensions": []
        },
        {
          "title": "type",
          "name": "type",
          "type": "string",
          "default": "_madeToOrder",
          "hidden": true
        },
        {
          "title": "Slug",
          "name": "slug",
          "type": "slug",
          "editable": true
        }
      ]
    },
    {
      "name": "_posts",
      "pageBundle": false,
      "previewPath": "recipes",
      "template": "[[workspace]]/.frontmatter/templates/_posts.mdx",
      "fields": [
        {
          "title": "Title",
          "name": "title",
          "type": "string",
          "single": true
        },
        {
          "title": "Subtitle",
          "name": "subtitle",
          "type": "string",
          "single": true
        },
        {
          "title": "Author",
          "type": "fields",
          "name": "author",
          "fields": [
            {
              "title": "Author Name",
              "name": "name",
              "type": "dataFile",
              "dataFileId": "teamMembers",
              "dataFileKey": "name",
              "dataFileValue": "name",
              "multiple": false
            },
            {
              "title": "Author Slug",
              "name": "slug",
              "type": "dataFile",
              "dataFileId": "teamMembers",
              "dataFileKey": "slug",
              "dataFileValue": "name",
              "multiple": false
            }
          ]
        },
        {
          "title": "Publishing date",
          "name": "date",
          "type": "string",
          "default": "{{now}} ",
          "single": true,
          "isPublishDate": true
        },
        {
          "title": "Cover Image",
          "name": "coverImage",
          "type": "image"
        },
        {
          "title": "Video Header Link",
          "name": "video",
          "type": "string",
          "default": "",
          "single": true
        },
        {
          "title": "Is in draft",
          "name": "draft",
          "type": "draft",
          "default": true
        },
        {
          "title": "Categories",
          "name": "categories",
          "type": "categories",
          "taxonomyLimit": 1
        },
        {
          "title": "Tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "Slug",
          "name": "slug",
          "type": "slug",
          "editable": true
        },
        {
          "title": "excerpt",
          "name": "excerpt",
          "type": "string"
        }
      ]
    },
    {
      "name": "_projectFiles",
      "pageBundle": false,
      "fields": [
        {
          "title": "title",
          "name": "title",
          "type": "string",
          "single": true
        },
        {
          "title": "date",
          "name": "date",
          "type": "datetime"
        },
        {
          "title": "active",
          "name": "active",
          "type": "draft"
        },
        {
          "title": "category",
          "name": "category",
          "type": "string",
          "single": true
        },
        {
          "title": "downloadLink",
          "name": "downloadLink",
          "type": "string",
          "single": true
        },
        {
          "title": "fileName",
          "name": "fileName",
          "type": "string",
          "single": true
        },
        {
          "title": "fileSize",
          "name": "fileSize",
          "type": "string",
          "single": true
        },
        {
          "title": "fileType",
          "name": "fileType",
          "type": "string",
          "single": true
        },
        {
          "title": "tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "coverImage",
          "name": "coverImage",
          "type": "string",
          "single": true
        },
        {
          "title": "videoCoverImage",
          "name": "videoCoverImage",
          "type": "boolean"
        },
        {
          "title": "excerpt",
          "name": "excerpt",
          "type": "string"
        },
        {
          "name": "type",
          "type": "string",
          "default": "_projectFile",
          "hidden": true
        }
      ]
    }
  ],
  "frontMatter.preview.host": "http://localhost:3000",
  "frontMatter.framework.id": "next",
  "frontMatter.content.publicFolder": "public",
  "frontMatter.content.pageFolders": [
