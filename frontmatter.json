  "frontMatter.content.snippets": {
    "Video": {
      "description": "video content",
      "body": "<Video url={'[[url]]'} poster={'[[poster]]'} autoplay={[[autoplay]]} title={'[[title]]'} muted={[[muted]]} loop={[[loop]]} controls={[[controls]]} playsInline={[[playsinline]]} />",
      "fields": [
        {
          "name": "url",
          "title": "Video URL",
          "type": "string",
          "default": "",
          "single": true
        },
        {
          "name": "poster",
          "title": "Poster Image",
          "type": "image"
        },
        {
          "name": "title",
          "title": "Title (optional)",
          "type": "string",
          "single": true
        },
        {
          "name": "muted",
          "title": "Muted",
          "type": "boolean",
          "default": true
        },
        {
          "name": "loop",
          "title": "Loop",
          "type": "boolean",
          "default": true
        },
        {
          "name": "controls",
          "title": "Show Controls",
          "type": "boolean",
          "default": false
        },
        {
          "name": "playsInline",
          "title": "Plays Inline",
          "type": "boolean",
          "default": true
        },
        {
          "name": "autoplay",
          "title": "Auto Play",
          "type": "boolean",
          "default": true
        }
      ]
    }
  },
  "frontMatter.data.files": [
    {
      "title": "All sponsors",
      "id": "all-sponsors",
      "file": "[[workspace]]/.frontmatter/content/sponsors.json",
      "labelField": "name",
      "type": "sponsors"
    },
    {
      "id": "teamMembers",
      "title": "Team Members",
      "file": "[[workspace]]/.frontmatter/content/people.json",
      "fileType": "json",
      "labelField": "name",
      "schema": {
        "title": "Team Members",
        "type": "object",
        "required": [
          "name",
          "slug",
          "addLink"
        ],
        "properties": {
          "name": {
            "type": "string",
            "title": "Name"
          },
          "slug": {
            "type": "string",
            "title": "Slug"
          },
          "addLink": {
            "type": "boolean",
            "title": "Link to Bio",
            "default": true
          }
        }
      }
    }
  ],
  "frontMatter.taxonomy.fieldGroups": [
    {
      "id": "credits",
      "labelField": "name",
      "fields": [
        {
          "title": "Name",
          "name": "name",
          "type": "string",
          "single": true
        },
        {
          "title": "Title",
          "name": "title",
          "type": "string",
          "single": true
        },
        {
          "title": "Website",
          "description": "Internal or otherwise.",
          "name": "website",
          "type": "string",
          "default": "/about/",
          "single": true
        }
      ]
    },
    {
      "id": "socials",
      "labelField": "poop",
      "fields": [
        {
          "title": "Platform",
          "name": "platform",
          "type": "choice",
          "choices": [
            "",
            "Instagram",
            "Facebook",
            "LinkedIn"
          ]
        },
        {
